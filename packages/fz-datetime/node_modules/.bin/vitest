#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.1_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@27.0.0_postcss@8.5.6__lightningcss@1.30.1_tsx@4.20.5/node_modules/vitest/node_modules:/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.1_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@27.0.0_postcss@8.5.6__lightningcss@1.30.1_tsx@4.20.5/node_modules:/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.1_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@27.0.0_postcss@8.5.6__lightningcss@1.30.1_tsx@4.20.5/node_modules/vitest/node_modules:/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/vitest@3.2.4_@types+node@22.18.1_@vitest+ui@3.2.4_jiti@2.5.1_jsdom@27.0.0_postcss@8.5.6__lightningcss@1.30.1_tsx@4.20.5/node_modules:/home/sammaji/wksp/saas/budgetbee-mono/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
